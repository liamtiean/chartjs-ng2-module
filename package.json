{
  "name": "ng2-chartjs",
  "version": "0.0.0",
  "description": "Angular2 ChartJS module",
  "private": true,
  "scripts": {
    "demo.gh-pages": "run-s build demo.build demo.deploy",
    "demo.ng-build": "ng build --prod --aot --preserve-symlinks -nc",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo.build": "npm run build && npm run link && npm run demo.ng-build",
    "link": "ngm link -p src --here",
    "lint": "tslint src/**/*.ts src/*.ts --exclude src/**/*.spec.ts src/*.spec.ts",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "build": "npm run lint && npm run build.ngm && npm run link",
    "build.ngm": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot"
  },
  "main": "bundles/ng2-chartjs.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "ng",
    "ng2",
    "Chartjs"
  ],
  "author": "Tien Tran",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh@github.com:liamtiean/ng2-chartjs.git"
  },
  "bugs": {
    "url": "https://github.com/liamtiean/ng2-chartjs/issues"
  },
  "homepage": "https://github.com/liamtiean/ng2-chartjs",
  "dependencies": {
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "^7.2.2",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@types/bootstrap": "^4.2.0",
    "@types/jquery": "^3.3.29",
    "@types/node": "~8.9.4",
    "@types/webpack": "4.1.6",
    "bootstrap": "^4.2.1",
    "classlist-polyfill": "1.2.0",
    "codelyzer": "~4.5.0",
    "core-js": "^2.6.2",
    "cpy": "5.1.0",
    "cpy-cli": "1.0.1",
    "gitignore-to-glob": "0.3.0",
    "jquery": "^3.3.1",
    "ngm-cli": "1.0.4",
    "reflect-metadata": "0.1.10",
    "require-dir": "0.3.2",
    "rxjs": "^6.3.3",
    "ts-helpers": "^1.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-valorsoft": "2.1.1",
    "typescript": "~3.2.2",
    "webdriver-manager": "12.0.6",
    "zone.js": "^0.8.28"
  }
}
